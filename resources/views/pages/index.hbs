{{#contentFor 'seoComponents'}}
  <meta
    name='description'
    content='PELUANG BISNIS ONLINE TERBARU PALING MUDAH DAN PALING MENGUNTUNGKAN RAHASIA MENDAPATKAN PASIF INCOME PULUHAN JUTA PERBULAN DARI MARKETPLACE TANPA RIBET, MODAL COPY PASTE'
  />
  <meta
    name='keywords'
    content='fingo,sukses,e-commerce,copy,paste,mudah,jutaan,income,bisnis,online'
  />
{{/contentFor}}

{{#contentFor 'pageStyle'}}
  <style>
    #navbar {
    transition-timing-function: 'ease-in-out';
    transition-duration: 500ms;
  }
  </style>
{{/contentFor}}

{{#contentFor 'pageScript'}}
  <script>
    var navbar = $('#navbar')
    var judul = $('#judul').height()

    addEventListener('scroll', function () {
      var scrollY = $(window).scrollTop()

      if (scrollY > judul) navbar.removeClass('navbar-dark bg-danger').addClass('navbar-light bg-light shadow shadow-sm')
      else navbar.removeClass('navbar-light bg-light shadow shadow-sm').addClass('navbar-dark bg-danger')
    })
  </script>
{{/contentFor}}

