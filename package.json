{
  "name": "client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "concurrently \"npm:development\" \"npm:watch\"",
    "build": "npm run webpack:production",
    "watch": "npm run webpack:development -- --watch",
    "development": "nodemon ./bin/www",
    "webpack:development": "cross-env NODE_ENV=development webpack --config ./config/webpack.config.js --progress --hide-modules",
    "webpack:production": "cross-env NODE_ENV=production webpack --progress ./config/webpack.config.js --hide-modules"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-hbs": "^2.3.3",
    "hbs": "^4.1.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.20",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.19",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "popper.js": "^1.16.1",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "swiper": "^6.0.2",
    "typed.js": "^2.0.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
